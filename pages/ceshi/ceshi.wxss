page{ width: 100%; height: 100%; overflow-x: hidden; background: #ebebeb;}
.merberBox{ width: 100%; height: 100%;}
.merber_title{ display: flex; box-sizing: border-box; height: 235rpx; display: -webkit-flex; flex-direction: column; align-items: center; padding: 15rpx 0; color: #fff; background:#282a3f; font-size: 32rpx; position: relative;}
.merber_bot{ width: 100%; height: 58rpx; position: absolute; left: 0px; bottom: 0px;}
.merber_bot image{ width: 100%; height: 100%;}
.avatarUrl{ width: 130rpx; height: 130rpx; margin: 5rpx auto 20rpx;}
.avatarUrl image{ width: 100%; height: 100%; border-radius: 15rpx;}
.merber_cont{ margin-top:15rpx; width: 100%; overflow: hidden;}
.item-cont{ width: 100%; background: #fff; box-sizing: border-box; padding: 0px 32rpx; border-bottom:1px solid #dadada; height: 82rpx; display: flex; display: -webkit-flex; align-items: center; position:relative;}
.item-cont-img{ width: 100rpx; height: 100rpx; margin-right: 10rpx; display: flex; display: -webkit-flex;  align-items: center; justify-content: center; position: absolute; left: 2%;}
.item-cont-img image{height: 55%; width: 55%;}
.item-cont-text{width: 300rpx; font-size:28rpx; padding-left: 75rpx;}
.item-cont-name{width: 280rpx; padding-right: 20rpx; font-size:28rpx; text-align: right; color:#999;
white-space: nowrap; text-overflow:ellipsis; overflow:hidden;}
.item-cont-icon{ text-align: right; height: 28rpx; line-height: 0px; flex: 1;}
.item-cont-icon image{ height: 35rpx; width: 35rpx;}
.item-cont{ position: relative;}
.item-cont-btn{ position: absolute; z-index: 2; width: 100%; height: 100%; border: 0px; background: none;}
button::after{border: 0px}
.btn-hover{ opacity: .8}
.shareBtn{ width: 80%; margin:auto; padding: 5rpx 0px; background: #fff; font-size: 32rpx;}
.shareWrap{ padding: 35rpx 0px;}
.fgx_item{ width: 100%; height: 15rpx; background: #ebebeb;}
.item-wrap{ background: #fff; display: flex; display: -webkit-flex; flex-direction:row; justify-content:space-around; margin-bottom:15rpx; padding: 25rpx 0px;}
.item-view{ display: flex; display: -webkit-flex; flex-direction:column; justify-content:space-around; align-items: center}
.item-view image{ display: block; width: 73rpx; height: 73rpx;}
.item-view text{ font-size: 27rpx;}
.version{ display: block; text-align: center; font-size: 18rpx;}