page{ height: 100%; background: #ebebeb;}
.ListBox{ width: 100%; box-sizing: border-box; padding: 0px 20rpx;}
.SearchBox{ width: 100%; background: #fff; box-sizing: border-box; padding: 18rpx 35rpx; display: flex; display: -webkit-flex; flex-direction: row;}
.SearchInput{ border:1px solid #acacac; height: 55rpx; line-height: 55rpx; border-radius: 28rpx; position: relative; width: 90%; padding: 0px 5%;}
.Icon{ width: 39rpx; height: 39rpx; position: absolute; left: 20rpx; top: 8rpx;}
.Icon image{ width: 100%; height: 100%; display: block;}
.SearchInput input{ height: 100%; width: 90%; box-sizing: border-box; padding-left: 65rpx; font-size: 30rpx; color: #666;}
.end{ flex: 1; text-align: center; line-height: 55rpx; font-size:32rpx;}
.text{ font-size: 32rpx;}
.close{ position: absolute; display: block; right: 20rpx; top: 8rpx; z-index: 99999;}
.Items{ width: 96%; position: relative; margin: 20rpx auto; background: #fff; border-radius: 5px; box-shadow: 0px 1px 1px rgba(0, 0, 0, 0.1); overflow:hidden;}
.Items_image{ width: 100%; height: 320rpx; display: block;}
.company{ display: block; width: 100%; text-align: right; font-size: 32rpx; line-height: 45rpx; height: 45rpx; overflow: hidden; box-sizing: border-box; padding: 0px 15rpx; color:#fff; position: absolute; left: 0px; bottom:20%; 
  background: -webkit-linear-gradient(rgba(0,0,0,0), rgba(0,0,0,0.2)); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(rgba(0,0,0,0), rgba(0,0,0,0.2)); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(rgba(0,0,0,0), rgba(0,0,0,0.2)); /* Firefox 3.6 - 15 */
  background: linear-gradient(rgba(0,0,0,0), rgba(0,0,0,0.2)); /* 标准的语法 */
}
.merber{border-top:1px solid #dadada; line-height: 80rpx; display: flex; display: -webkit-flex; flex-direction: row; align-items: center; box-sizing: border-box; padding: 0rpx 15rpx; }
.merber-left{display: flex; display: -webkit-flex; flex-direction: row; align-items: center; font-size:28rpx; color:#666;}
.merber-left image{ width: 50rpx; height: 50rpx; margin-right: 15rpx;}
.merber-right{ flex: 1; text-align: right; color:#8f8f8f; font-size:28rpx;}
.operation{ position: absolute; left: 45rpx; top: 20rpx; color: #fff; font-size: 28rpx; z-index: 99; display: flex; display: -webkit-flex; flex-direction: row; align-items: center;}
.operation image{ height: 32rpx; width: 40rpx; height: 26rpx; margin-right: 15rpx;}
.operation text{ margin-right:25rpx;}
.ewm{ width: 45rpx; height: 45rpx; position: absolute; right: 25rpx; top: 25rpx; z-index: 99}
.ewm image{ width: 100%; height: 100%;}
.title{ color:#fff; line-height: 52rpx; position: absolute; top: 17%; left: 45rpx;  right: 45rpx; z-index: 99; font-size: 42rpx;}
.canvas{ width: 750rpx; height: 854rpx; display: block; opacity: 0; position: fixed; left: 100%; top: 100%;}
.ewmimg image{ width: 100rpx; height: 100rpx;}
.loader{ width: 100%; text-align: center; padding: 20rpx 0px; color:#666; font-size:32rpx;}
.newIcon{ position: absolute; width: 47rpx; height: 47rpx; top: 0; left: 0;}
.scbtn{ width: 38rpx; height: 36rpx; position: absolute; right: 15%; top: 20rpx;}
.scbtn image{ width: 100%; height: 100%;}